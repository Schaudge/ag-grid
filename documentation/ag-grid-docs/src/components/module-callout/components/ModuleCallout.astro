---
import { ModuleCallout } from '@components/module-callout/components/ModuleCallout';
import { getFrameworkFromPath } from '@components/docs/utils/urlPaths';

interface Props {
    moduleName: string;
}

const { pathname } = Astro.url;
const { moduleName } = Astro.props as Props;
const framework = getFrameworkFromPath(pathname);
---

<ModuleCallout moduleName={moduleName} framework={framework} client:load />
